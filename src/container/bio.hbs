<div>
    <div>
        <header>
            <div>
                <h1>{{name}}</h1>
            </div>
            <div><strong>Birthdate: </strong>{{birthdate}}</div>
            <div><strong>Birthplace: </strong>{{birthplace}} </div>
            <div><strong>Deathdate: </strong>{{deathdate}}</div>
            <div><strong>Deathplace: </strong> {{deathplace}} </div>
            <div><strong>Nationality: </strong>{{nationality}}</div>
            <div><strong>Occupation: </strong>{{occupation}}</div>
            <div><strong>Known for: </strong>
                <ul>
                    {{#each known_for}}
                    <li>
                        <q>{{this}}</q>
                    </li>
                    {{/each}}
                </ul>
            </div>

        </header>

        <hr>
        <br>

        <main>
            <div>
                {{#each education}}
                <div><strong>Education: </strong></div>
                <div><b>Degree: </b>{{degree}}</div>
                <div><b>Field: </b>{{field}}</div>
                <div><b>Institution: </b>{{institution}}</div>
                <div><b>Year: </b>{{year}}</div>
                {{/each}}
            </div>

            <br>

            <div>
                <div><strong>Notable publications: </strong></div>
                {{#each notable_publications}}
                <div><q>{{title}}</q></div>
                <div><b>Year </b>{{year}}</div>
                <div><b>Publisher: </b>{{publisher}}</div>
                <br>
                {{/each}}
            </div>

            <br>

            <div>
                <div><strong>Partners: </strong></div>
                {{#each partners}}
                <div>{{name}}</div>
                <div><b>Relationship: </b><q>{{relationship}}</q></div>
                <div><b>Years: </b>{{years}}</div>
                <br>
                {{/each}}
            </div>

            <br>

            <div>
                <div><strong>Awards: </strong></div>
                {{#each awards}}
                <div>{{title}}</div>
                <div><b>Years: </b>{{years}}</div>
                <div><b>Description: </b><q>{{description}}</q></div>
                {{/each}}
            </div>

            <br>

            <div>
                <div><strong>Influences: </strong></div>
                <ul>
                    {{#each influences}}
                    <li>
                        {{this}}
                    </li>
                    {{/each}}
                </ul>
            </div>

            <br>

            <div>
                <div><strong>Influenced: </strong></div>
                <ul>
                    {{#each influenced}}
                    <li>
                        {{this}}
                    </li>
                    {{/each}}
                </ul>
            </div>

            <br>

            <div>
                <div><strong>Quotes: </strong></div>
                <ul>
                    {{#each quotes}}
                    <li>
                        <q>{{this}}</q>
                    </li>
                    {{/each}}
                </ul>
            </div>

            <br>

            <div>
                <div><strong>Major discoveries: </strong></div>
                {{#each major_discoveries}}
                <div>{{title}}</div>
                <div><b>Years: </b>{{years}}</div>
                <div><b>Description: </b><q>{{description}}</q></div>
                {{/each}}
            </div>

            <br>

            <div>
                <div><strong>Contributions: </strong></div>
                {{#with contributions}}
                <div>{{title}}</div>
                <div><b>Description: </b><q>{{description}}</q></div>
                <div><b>Year: </b><q>{{year}}</q></div>
                <div><b>Field: </b>{{field}}</div>
                {{/with}}
            </div>

        </main>


