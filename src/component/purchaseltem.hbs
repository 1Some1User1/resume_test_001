<div>
    <div class="col-12">
        <div class="row">
            {{#each itemList}}
            <div class="row">
                <div class="col-4">
                    <p>{{number}}</p>
                    <div>
                        <big><b>{{status}}</b></big>
                    </div>
                </div>
                <div class="col-2">
                    {{#with amount}}
                    <div>{{title}}</div>
                    <div class="pt-3">{{value}}</div>
                    {{/with}}
                </div>
                <div class="col-4">
                    <div class="row">
                        {{#each images}}
                        <div class="col-4">
                            <img class="card-img-top" src="{{this}}" alt="Product photo">
                        </div>
                        {{/each}}
                    </div>
                </div>
                <div class="col-2">
                    {{#with button}}
                    <a href="{{link}}" class="btn btn-{{#if isPrimary}}primary{{/if}}">{{text}}</a>
                    {{/with}}
                </div>
                <div>
                    <hr>
                </div>
            </div>
            {{/each}}
        </div>
    </div>
</div>